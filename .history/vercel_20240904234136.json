{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        },
        "dest": "/dist/main.js"
      }
    ]
  }
  